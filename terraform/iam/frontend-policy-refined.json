{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3StaticHostingObjects",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::getting-started-frontend-bucket"
      ]
    },
    {
      "Sid": "S3StaticHostingObjectsPutGet",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::getting-started-frontend-bucket/*"
      ]
    },
    {
      "Sid": "CloudFrontInvalidation",
      "Effect": "Allow",
      "Action": [
        "cloudfront:CreateInvalidation",
        "cloudfront:GetDistribution",
        "cloudfront:GetInvalidation"
      ],
      "Resource": "arn:aws:cloudfront::618889059366:distribution/*"
    }
  ]
}
